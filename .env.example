# Huly MCP Server Configuration
# Copy this file to .env and fill in your actual credentials

# Huly instance URL (without trailing slash)
HULY_URL=https://your-huly-instance.com

# Your Huly login credentials
HULY_EMAIL=your.email@example.com
HULY_PASSWORD=your-password-here

# Workspace name to connect to
HULY_WORKSPACE=your-workspace-name
